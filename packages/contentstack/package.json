{
  "name": "@contentstack/cli",
  "description": "Command-line tool (CLI) to interact with Contentstack",
  "version": "2.0.0-beta.10",
  "author": "Contentstack",
  "bin": {
    "csdx": "./bin/run.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./lib ./node_modules tsconfig.build.tsbuildinfo",
    "compile": "tsc -b tsconfig.json",
    "postpack": "rm -f oclif.manifest.json",
    "version": "oclif readme  && git add README.md",
    "test:report": "tsc -p test && nyc --reporter=lcov --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "pretest": "tsc -p test",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "npm run lint",
    "lint": "eslint src/**/*.ts",
    "prepack": "pnpm compile && oclif manifest && oclif readme"
  },
  "dependencies": {
    "@contentstack/cli-audit": "~2.0.0-beta.2",
    "@contentstack/cli-cm-export": "~2.0.0-beta.7",
    "@contentstack/cli-cm-import": "~2.0.0-beta.6",
    "@contentstack/cli-auth": "~2.0.0-beta.4",
    "@contentstack/cli-bulk-operations": "^1.0.0-beta",
    "@contentstack/cli-cm-bootstrap": "~2.0.0-beta.6",
    "@contentstack/cli-cm-branches": "~1.6.3",
    "@contentstack/cli-cm-clone": "~2.0.0-beta.8",
    "@contentstack/cli-cm-export-to-csv": "~1.11.0",
    "@contentstack/cli-cm-import-setup": "~2.0.0-beta.3",
    "@contentstack/cli-cm-seed": "~2.0.0-beta.6",
    "@contentstack/cli-command": "~1.7.2",
    "@contentstack/cli-config": "~1.19.0",
    "@contentstack/cli-launch": "^1.9.6",
    "@contentstack/cli-migration": "~2.0.0-beta.3",
    "@contentstack/cli-utilities": "~1.17.2",
    "@contentstack/cli-variants": "~2.0.0-beta.4",
    "@contentstack/management": "~1.27.5",
    "@contentstack/utils": "~1.7.0",
    "@oclif/core": "^4.8.0",
    "@oclif/plugin-help": "^6.2.37",
    "@oclif/plugin-not-found": "^3.2.74",
    "@oclif/plugin-plugins": "^5.4.56",
    "chalk": "^4.1.2",
    "cli-progress": "^3.12.0",
    "debug": "^4.4.3",
    "figlet": "1.8.2",
    "inquirer": "8.2.7",
    "node-machine-id": "^1.1.12",
    "open": "^8.4.2",
    "ora": "^8.2.0",
    "semver": "^7.7.4",
    "short-uuid": "^4.2.2",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@oclif/test": "^4.1.16",
    "@types/chai": "^4.3.20",
    "@types/inquirer": "^9.0.8",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^8.2.3",
    "@types/node": "^14.18.63",
    "@types/semver": "^7.7.0",
    "@types/sinon": "^10.0.20",
    "chai": "^4.5.0",
    "eslint": "^8.57.1",
    "eslint-config-oclif": "^6.0.62",
    "eslint-config-oclif-typescript": "^3.1.14",
    "globby": "^10.0.2",
    "mocha": "10.8.2",
    "nock": "^13.5.6",
    "nyc": "^15.1.0",
    "oclif": "^4.22.77",
    "rimraf": "^5.0.10",
    "shelljs": "^0.10.0",
    "sinon": "^21.0.1",
    "tmp": "^0.2.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/contentstack/cli",
  "keywords": [
    "contentstack",
    "cli",
    "plugin"
  ],
  "license": "MIT",
  "oclif": {
    "topicSeparator": ":",
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "topics": {
      "auth": {
        "description": "Perform authentication-related activities"
      },
      "cm": {
        "description": "Perform content management activities"
      },
      "config": {
        "description": "Perform configuration related activities"
      },
      "cm:assets": {
        "description": "Assets related operations"
      },
      "cm:entries": {
        "description": "Entries related operations"
      },
      "cm:stacks": {
        "description": "Stacks related operations"
      },
      "launch": {
        "description": "Launch related operations"
      },
      "plugins": {
        "description": "List installed plugins"
      }
    },
    "helpClass": "./lib/help.js",
    "bin": "csdx",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-plugins",
      "@contentstack/cli-config",
      "@contentstack/cli-auth",
      "@contentstack/cli-cm-import",
      "@contentstack/cli-cm-export",
      "@contentstack/cli-cm-seed",
      "@contentstack/cli-cm-bootstrap",
      "@contentstack/cli-cm-export-to-csv",
      "@contentstack/cli-cm-clone",
      "@contentstack/cli-migration",
      "@contentstack/cli-cm-branches",
      "@contentstack/cli-audit",
      "@contentstack/cli-cm-import-setup",
      "@contentstack/cli-launch",
      "@contentstack/cli-bulk-operations"
    ],
    "hooks": {
      "prerun": [
        "./lib/hooks/prerun/default-rate-limit-check",
        "./lib/hooks/prerun/latest-version-warning"
      ],
      "init": [
        "./lib/hooks/init/context-init",
        "./lib/hooks/init/utils-init"
      ]
    }
  },
  "repository": "https://github.com/contentstack/cli"
}