{
  "name": "@contentstack/cli-config",
  "description": "Contentstack CLI plugin for configuration",
  "version": "1.0.0",
  "author": "Contentstack",
  "bin": {
    "csdx": "./bin/run"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./lib && rm -rf tsconfig.build.tsbuildinfo",
    "compile": "tsc -b tsconfig.json",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md",
    "test:report": "tsc -p test && nyc --reporter=lcov --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "pretest": "tsc -p test",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "npm run lint",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "test:integration": "mocha --forbid-only \"test/run.test.ts\" --integration-test",
    "test:unit": "mocha --forbid-only \"test/unit/*.test.ts\" --unit-test"
  },
  "dependencies": {
    "@contentstack/cli-command": "^1.0.0",
    "@contentstack/cli-utilities": "^1.0.0",
    "chalk": "^4.0.0",
    "debug": "^4.1.1",
    "inquirer": "^7.1.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "oclif": "^3.1.2",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.18",
    "@types/inquirer": "^7.3.1",
    "@types/mkdirp": "^1.0.1",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.32",
    "@types/sinon": "^10.0.2",
    "@types/tar": "^4.0.3",
    "chai": "^4.3.4",
    "eslint": "^8.18.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "rimraf": "^2.7.1",
    "sinon": "^11.1.1",
    "tmp": "^0.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/contentstack/cli",
  "keywords": [
    "contentstack",
    "cli",
    "plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "csdx",
    "protected": [
      "config:get:region"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": "contentstack/cli"
}