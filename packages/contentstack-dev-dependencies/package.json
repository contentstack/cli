{
  "name": "@contentstack/cli-dev-dependencies",
  "version": "1.0.0",
  "description": "Customized dev dependencies for ContentStack projects",
  "main": "lib/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "prepack": "npm run clean && npm run compile",
    "clean": "rm -rf ./lib && rm -rf tsconfig.build.tsbuildinfo",
    "compile": "tsc -b tsconfig.json",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix"
  },
  "repository": "contentstack/cli",
  "keywords": [
    "contentstack-dependencies",
    "test-suite",
    "csdx-test-suite",
    "dev-dependencies"
  ],
  "author": "contentstack",
  "license": "MIT",
  "dependencies": {
    "@oclif/core": "^1.16.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.32",
    "eslint": "^8.18.0",
    "fancy-test": "^2.0.0",
    "mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "files": [
    "/lib"
  ]
}
