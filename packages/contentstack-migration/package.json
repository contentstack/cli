{
  "name": "@contentstack/cli-migration",
  "version": "1.0.0",
  "author": "@contentstack",
  "bugs": "https://github.com/contentstack/cli/issues",
  "dependencies": {
    "@contentstack/cli-command": "^1.0.0",
    "@contentstack/management": "^1.3.0",
    "@contentstack/cli-utilities": "^1.0.0",
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.18.3",
    "async": "^3.2.4",
    "callsites": "^3.1.0",
    "cardinal": "^2.1.1",
    "chalk": "^5.0.1",
    "dot-object": "^2.1.4",
    "dotenv": "^8.2.0",
    "listr": "^0.14.3",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/test": "^1.2.8",
    "chai": "^4.3.4",
    "eslint": "^8.18.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^10.0.2",
    "husky": "^4.2.5",
    "jsdoc": "^3.6.7",
    "jsdoc-to-markdown": "^7.0.1",
    "mkdirp": "^1.0.4",
    "mocha": "^10.0.0",
    "nock": "^13.1.1",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "files": [
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/contentstack/cli",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./src/commands",
    "bin": "csdx",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "contentstack/cli-migration",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint ./src",
    "generate-api-ref-md-doc": "nyc jsdoc2md ./src/modules/* > ./docs/api-reference.md",
    "prepack": "oclif-dev manifest && oclif-dev readme && npm run generate-api-ref-md-doc",
    "test": "nyc mocha \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "csdxConfig": {
    "expiredCommands": {
      "cm:migration": "csdx cm:stacks:migration"
    }
  }
}
