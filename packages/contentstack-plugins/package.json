    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.25.1",
    "@oclif/plugin-help": "^3.2.0",
    "@oclif/test": "^1.2.8",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
