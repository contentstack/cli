{
  "name": "@contentstack/cli-cm-bootstrap",
  "description": "Bootstrap contentstack apps",
  "version": "1.0.0",
  "author": "Contentstack",
  "bugs": "https://github.com/contentstack/cli/issues",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./lib && rm -rf tsconfig.build.tsbuildinfo",
    "compile": "tsc -b tsconfig.json",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "test": "npm run build && npm run test:e2e",
    "test:e2e": "nyc mocha \"test/**/*.test.js\"",
    "test:report": "nyc --reporter=lcov mocha \"test/**/*.test.js\""
  },
  "dependencies": {
    "@contentstack/cli-command": "^0.1.1-beta.1",
    "@contentstack/cli-cm-seed": "1.0.6",
    "@contentstack/management": "^1.2.1",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "axios": "^0.21.1",
    "cli-ux": "^5.5.0",
    "inquirer": "^8.0.0",
    "mkdirp": "^1.0.4",
    "tar": "^6.0.5"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/plugin-help": "^3.2.0",
    "@oclif/test": "^1.2.8",
    "@types/inquirer": "^7.3.1",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.14.32",
    "@types/tar": "^4.0.3",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "rimraf": "^2.7.1",
    "tmp": "^0.2.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/contentstack/cli",
  "keywords": [
    "contentstack",
    "cli",
    "plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "csdx",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "plugin": [
      "@contentstack/cli-cm-seed"
    ]
  },
  "repository": "contentstack/cli"
}
