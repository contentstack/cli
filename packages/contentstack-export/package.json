{
  "name": "@contentstack/cli-cm-export",
  "description": "Contentstack CLI plugin to export content from stack",
  "version": "1.0.0",
  "author": "Contentstack",
  "bugs": "https://github.com/contentstack/cli/issues",
  "dependencies": {
    "@contentstack/cli-command": "^1.0.0",
    "@contentstack/cli-utilities": "^1.0.0",
    "@contentstack/management": "^1.3.0",
    "async": "^3.2.4",
    "bluebird": "^3.7.2",
    "fs": "0.0.1-security",
    "is-valid-path": "^0.1.1",
    "lodash": "^4.17.20",
    "mkdirp": "^1.0.4",
    "path": "^0.12.7",
    "progress-stream": "^2.0.0",
    "proxyquire": "^2.1.3",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "oclif": "^3.1.2",
    "@oclif/test": "^1.2.6",
    "assert": "^2.0.0",
    "chai": "^4.2.0",
    "eslint": "^8.18.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2",
    "mocha": "^10.0.0",
    "nyc": "^14.1.1",
    "sinon": "^9.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src",
    "/messages"
  ],
  "homepage": "https://github.com/contentstack/cli",
  "keywords": [
    "contentstack",
    "cli",
    "plugin"
  ],
  "license": "MIT",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif manifest && oclif readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "commands": "./src/commands",
    "bin": "csdx",
    "repositoryPrefix": "<%- repo %>/blob/v<%- version %>/packages/contentstack-export/<%- commandPath %>"
  },
  "csdxConfig": {
    "expiredCommands": {
      "cm:export": "csdx cm:stacks:export"
    }
  },
  "main": "./src/commands/cm/stacks/export.js",
  "repository": "https://github.com/contentstack/cli"
}