{
  "name": "@contentstack/cli-cm-export",
  "description": "Contentstack CLI plugin to export content from stack",
  "version": "1.2.3",
  "author": "Contentstack",
  "bugs": "https://github.com/contentstack/cli/issues",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./lib && rm -rf tsconfig.build.tsbuildinfo",
    "compile": "tsc -b tsconfig.json",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "npm run build && npm run oclif-paths manifest && npm run oclif-paths readme",
    "version": "oclif readme && git add README.md",
    "test:report": "tsc -p test && nyc --reporter=lcov --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "pretest": "tsc -p test",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register --forbid-only \"test/**/*.test.ts\"",
    "oclif-paths": "ts-node --project tsconfig.json -r tsconfig-paths/register node_modules/.bin/oclif",
    "posttest": "npm run lint",
    "format": "eslint src/**/*.ts --fix",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "test:integration": "mocha --forbid-only \"test/integration/*.test.ts\"",
    "test:unit": "mocha --forbid-only \"test/unit/*.test.ts\""
  },
  "dependencies": {
    "@contentstack/cli-command": "^1.0.4",
    "@contentstack/cli-utilities": "^1.0.5",
    "async": "^3.2.4",
    "big-json": "^3.2.0",
    "bluebird": "^3.7.2",
    "fs": "0.0.1-security",
    "is-valid-path": "^0.1.1",
    "lodash": "^4.17.20",
    "merge": "^2.1.1",
    "mkdirp": "^1.0.4",
    "path": "^0.12.7",
    "progress-stream": "^2.0.0",
    "promise-limit": "^2.7.0",
    "proxyquire": "^2.1.3",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/plugin-help": "^3.2.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.18",
    "@types/inquirer": "^7.3.1",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^8.2.2",
    "@types/node": "^16.18.7",
    "@types/sinon": "^10.0.2",
    "@types/tar": "^4.0.3",
    "@types/winston": "^2.4.4",
    "chai": "^4.3.4",
    "eslint": "^8.18.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "globby": "^10.0.2",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "oclif": "^3",
    "rimraf": "^2.7.1",
    "sinon": "^11.1.1",
    "tmp": "^0.2.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/src",
    "./lib",
    "./types",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/contentstack/cli",
  "keywords": [
    "contentstack",
    "cli",
    "plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "csdx",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": ":",
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ]
  },
  "repository": "contentstack/cli"
}
