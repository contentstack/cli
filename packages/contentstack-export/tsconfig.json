{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": false,
    "importHelpers": true,
    "module": "commonjs",
    "outDir": "lib",
    "strict": false,
    "target": "ES6",
    "allowJs": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "esModuleInterop": true,
    "strictPropertyInitialization": false,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@export/*": ["./src/*"],
      "@export/test/*": ["./test/*"],
      "@export/messages/*": ["./messages/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": [
    "node_modules",
    "lib",
    "typings/**"
  ]
}