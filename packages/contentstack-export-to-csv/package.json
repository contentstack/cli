{
  "name": "@contentstack/cli-cm-export-to-csv",
  "description": "Export entities to csv",
  "version": "1.0.2",
  "author": "Abhinav Gupta @abhinav-from-contentstack",
  "bugs": "https://github.com/contentstack/cli/issues",
  "dependencies": {
    "@contentstack/cli-command": "^1.0.2",
    "@contentstack/cli-utilities": "^1.0.3",
    "@contentstack/management": "^1.6.0",
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.18.3",
    "chalk": "^4.1.0",
    "fast-csv": "^4.3.6",
    "inquirer": "8.2.4",
    "inquirer-checkbox-plus-prompt": "^1.0.1",
    "mkdirp": "^1.0.4"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/test": "^1.2.8",
    "chai": "^4.2.0",
    "debug": "^4.3.1",
    "eslint": "^8.18.0",
    "eslint-config-oclif": "^4.0.0",
    "globby": "^10.0.2",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/contentstack/cli",
  "keywords": [
    "contentstack",
    "cli",
    "plugin"
  ],
  "scripts": {
    "pack": "npm pack && mv *.tgz ../../build",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "csdxConfig": {
    "expiredCommands": {}
  },
  "main": "./src/commands/cm/export-to-csv.js",
  "license": "MIT",
  "oclif": {
    "commands": "./src/commands",
    "bin": "csdx",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "repositoryPrefix": "<%- repo %>/blob/main/packages/contentstack-export-to-csv/<%- commandPath %>"
  },
  "repository": "https://github.com/contentstack/cli"
}
