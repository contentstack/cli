      "@oclif/plugin-help"
    ]
  },
  "csdxConfig": {
    "expiredCommands": {
      "cm:export-to-csv": "command will be deprecated in two months, start using cm:entries:export-to-csv instead"
    }
  },
  "repository": "contentstack/cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
